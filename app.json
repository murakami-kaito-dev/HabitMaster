{
  "expo": {
    "name": "HabitMaster",
    "slug": "HabitMaster",
    "version": "1.0.0",
    "scheme": "HabitMaster",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.km.habitmaster",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "ja",
        "CFBundleLocalizations": ["ja", "en"]
      }
    },
    "android": {
      "package": "com.km.habitmaster",
      "versionCode": 1,
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "8b0f5b0f-2742-4276-a461-a18482572922"
      }
    }
  }
}
